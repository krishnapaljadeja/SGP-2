    <body>
        <div id="webcrumbs">
            <div class="w-full flex justify-center">
                <div class="w-[1000px] bg-gray-50 min-h-screen">
                    <header class="bg-white shadow-sm py-3 px-6 flex justify-between items-center">
                        <div class="text-blue-600 font-bold text-2xl">QUIZIFY</div>
                        <div class="flex items-center gap-4">
                            <button class="hover:bg-gray-100 rounded-full p-2 transition-colors duration-200">
                                <span class="material-symbols-outlined">account_circle</span>
                            </button>
                            <button
                                class="bg-red-500 hover:bg-red-600 text-white px-4 py-1.5 rounded-full text-sm transition-all duration-200 hover:shadow-md"
                            >
                                Logout
                            </button>
                        </div>
                    </header>
                    <div class="p-6 max-w-4xl mx-auto">
                        <div class="bg-white rounded-xl shadow-sm p-6">
                            <div class="flex justify-center">
                                <div class="text-center mb-8">
                                    <div
                                        class="bg-green-100 text-green-700 rounded-full px-4 py-1.5 text-sm font-medium inline-block mb-2"
                                    >
                                        Quiz Completed
                                    </div>
                                    <h1 class="text-3xl font-bold text-gray-800 mb-2"><%=quiz.title%> Quiz Results</h1>
                                    <p class="text-gray-600">You have successfully completed this quiz!</p>
                                </div>
                            </div>
                            <div class="flex justify-center mb-10">
                                <div class="relative w-56 h-56">
                                    <div class="absolute inset-0 flex items-center justify-center">
                                        <div class="text-center">
                                            <div class="text-5xl font-bold text-blue-600"><%=totalScore%></div>
                                            <div class="text-gray-500 mt-1">out of <%= totalPossiblePoints %> points</div>
                                        </div>
                                    </div>
                                    <svg class="w-full h-full" viewBox="0 0 100 100">
                                        <circle
                                            class="text-gray-200"
                                            stroke-width="10"
                                            stroke="currentColor"
                                            fill="transparent"
                                            r="40"
                                            cx="50"
                                            cy="50"
                                        ></circle>
                                        <circle
                                            class="text-blue-600 transform -rotate-90 origin-center transition-all duration-1000"
                                            stroke-width="10"
                                            stroke-dasharray="251"
                                            stroke-dashoffset="45"
                                            stroke="currentColor"
                                            fill="transparent"
                                            r="40"
                                            cx="50"
                                            cy="50"
                                        ></circle>
                                    </svg>
                                </div>
                            </div>
                            <div class="bg-blue-50 rounded-xl p-6 mb-8">
                                <h2 class="text-xl font-semibold text-gray-800 mb-4">Quiz Summary</h2>
                                <div class="grid grid-cols-1 md:grid-cols-2 gap-4 mb-6">
                                    <div
                                        class="bg-white rounded-lg p-4 shadow-sm hover:shadow-md transition-shadow duration-200"
                                    >
                                        <div class="text-gray-600 mb-1">Total Questions</div>
                                        <div class="text-2xl font-bold">5</div>
                                    </div>
                                    <div
                                        class="bg-white rounded-lg p-4 shadow-sm hover:shadow-md transition-shadow duration-200"
                                    >
                                        <div class="text-gray-600 mb-1">Time Taken</div>
                                        <div class="text-2xl font-bold">11:15</div>
                                    </div>
                                    <div
                                        class="bg-white rounded-lg p-4 shadow-sm hover:shadow-md transition-shadow duration-200"
                                    >
                                        <div class="text-gray-600 mb-1">Correct Answers</div>
                                        <div class="text-2xl font-bold text-green-600">4</div>
                                    </div>
                                    <div
                                        class="bg-white rounded-lg p-4 shadow-sm hover:shadow-md transition-shadow duration-200"
                                    >
                                        <div class="text-gray-600 mb-1">Incorrect Answers</div>
                                        <div class="text-2xl font-bold text-red-500">1</div>
                                    </div>
                                </div>
                            </div>
                            <div class="flex justify-between items-center mt-8">
                                <button
                                    class="bg-white border border-blue-600 text-blue-600 hover:bg-blue-50 px-6 py-2.5 rounded-lg transition-all duration-200 hover:shadow-md flex items-center gap-2"
                                >
                                    <span class="material-symbols-outlined">arrow_back</span>
                                    <span>Back to Quizzes</span>
                                </button>
                                <div class="flex gap-4"></div>
                            </div>
                            <div class="mt-8 border-t border-gray-200 pt-6"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>
